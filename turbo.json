{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "web#build": {
      "cache": false
    },
    "client#build": {
      "dependsOn": ["^web#build"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^client#build"],
      "cache": false
    }
  }
}
