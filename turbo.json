{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "DISPLAY",
    "DBUS_SESSION_BUS_ADDRESS"
  ],
  "globalDependencies": [
    "**/.env.*local"
  ],
  "tasks": {
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "env": [
        "VITE_*"
      ],
      "persistent": true
    },
    "web#build": {
      "cache": false
    },
    "desktop#build": {
      "cache": false,
      "dependsOn": [
        "web#build"
      ]
    },
    "build": {
      "cache": false,
      "dependsOn": [
        "desktop#build"
      ]
    }
  }
}
